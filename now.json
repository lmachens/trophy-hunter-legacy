{
  "version": 2,
  "name": "th",
  "alias": ["th.gl", "www.th.gl"],
  "env": {
    "LEAGUE_API_KEY": "@league_api_key",
    "MONGODB_URI": "@mongodb_uri"
  },
  "builds": [
    { "src": "src/api/endpoints/**/*.ts", "use": "@now/node@canary" },
    { "src": "src/www/next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/src/api/endpoints/$1" },
    { "src": "/(.*)", "dest": "/src/www/$1" }
  ]
}
